    "@angular/core": "^8.2.10",
    "@angular/platform-browser": "^8.2.10",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "typescript": "~3.5.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular/common": "^8.2.10",
    "@angular/compiler": "^8.2.10",
    "@angular/compiler-cli": "^8.2.10",
    "@angular/core": "^8.2.10",
    "@angular/platform-browser": "^8.2.10",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "tslint": "^5.20.0",
    "typescript": "~3.5.3",
    "zone.js": "~0.9.1"
  }
